{
    "name": "backend-opengraph-webshot",
    "version": "0.2.0",
    "license": "MIT",
    "module": ".dist/esm/server.js",
    "main": ".dist/cjs/server.js",
    "private": true,
    "files": [
        ".dist"
    ],
    "scripts": {
        "start": "node .dist/cjs/server",
        "dev": "tsnd -r dotenv/config --respawn ./src/server.ts",
        "clean": "yarn g:tsc:clean",
        "build:project": "yarn g:tsc:project",
        "build:cjs": "yarn g:tsc:cjs",
        "build": "yarn build:cjs"
    },
    "dependencies": {
        "@aelea/core": "1.3.0",
        "@most/core": "1.6.1",
        "@most/disposable": "1.3.0",
        "@most/prelude": "1.8.0",
        "@most/scheduler": "1.3.0",
        "@most/types": "1.1.0",
        "concurrently": "^7.0.0",
        "dotenv": "^11.0.0",
        "express": "^4.17.2",
        "express-promise-router": "^4.1.1",
        "express-session": "^1.17.2",
        "puppeteer": "13.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/express-serve-static-core": "^4.17.28",
        "@types/express-session": "^1.17.4",
        "@types/node": "17.0.8",
        "@types/ws": "^8.2.2",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./app/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    }
